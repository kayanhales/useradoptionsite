  body.theme--purple
    .nav.has-no-secondary
      a.nav__brand(href='/')
        img.nav__logo(src='http://assets.dynatrace.com/global/logos/dynatrace-logo.svg', alt='dynatrace logo')
      .nav__buttongroup
        a.nav__btn.nav__btn--cta(href='https://github.com/Dynatrace/css-groundhog') Find me on GitHub
        button#menu-toggle.nav__btn--menutoggle.nav__btn(data-target='#nav-bar') Menu
      nav#nav-bar.nav__bar.js-nav-toggle
        ul.nav__list.nav__list--primary
          li.nav__item
            a.nav__link(href='/doc/components/') DXS
          li.nav__item
            a.nav__link(href='/doc/layouts/') Service Plans
          li.nav__item
            a.nav__link(href='/doc/test-pages/') User Adoption Plans
          li.nav__item
